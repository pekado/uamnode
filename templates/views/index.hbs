</div>
					
	
	

	
	<div class="section fullscreen" id="contacto" style="display: inline-flex;">
		<img class="mise padding" style="float: left;" src="imgs/miselaneas.png">

		<div class="contacto_col_right" id="anima12">
			

			<div class="form">
				
					
				<h4 class="clr_main" id="ayudarte">ESTAMOS<br> PARA AYUDARTE</h4>
				
				<form align="center" method="post" name="form" action="#contacto">
					 
					<input type="text" name="nombre" maxlength="90" placeholder="NOMBRE" required />
					<input type="text" name="apellido" maxlength="90" placeholder="APELLIDO" style="float: right;" required />
					<input type="email" name="email" maxlength="90" placeholder="E-MAIL" required />
					<input type="text" name="telefono" maxlength="90" placeholder="TEL&Eacute;FONO" style="float: right;"  />
					<textarea name="mensaje" placeholder="MENSAJE" required></textarea>
					<button class="boton" type="submit">ENVIAR ></button>
					
				</form>

			</div>
			<a style="font-size: 40px;" href="https://api.whatsapp.com/send?phone=" target="_blank" class="fa fa-whatsapp"></a>
		</div>	
		<img class="mise padding" style="float: right;" src="imgs/miselaneas2.png">
	</div>

		
	
	<div class="section" id="map"> </div>
	
	

	<script src="js/jquery-1.7.2.min.js" type="text/javascript"></script>
	<script src="js/fontawesome.js" type="text/javascript"></script>
	<script src="slick/slick.min.js" type="text/javascript" ></script>
	<script src="js/waypoint.js" type="text/javascript"></script>
	<script src="js/scripts.js" type="text/javascript"></script>
	<script src="js/anima.js" type="text/javascript"></script>
	<script src="js/jquery.fancybox.pack.js" type="text/javascript"></script>

	<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyANB--3I9cvZo-ERw-YfU0JQc9fPkzl2OQ&amp;callback=initialize">  </script>
	
	<script type="text/javascript">

		jQuery(document).ready(function() {			

			var pag_items = 1;
			var numpags_items = 3;
			var total_items = 7;
	
			jQuery(".items_pag1").show();
	
			jQuery(".activa_items").click(function(event) {
	
				event.preventDefault();
				var rel = jQuery(this).attr('rel');
				
				for(i=1;i<=numpags_items;i++) { jQuery('.items_pag'+i).hide(); }
				
				if(rel==1) if(pag_items<numpags_items) pag_items++; else pag_items = 1;
				else if(pag_items>1) pag_items--; else pag_items = numpags_items;
				
				$(".items_pag"+pag_items).sort(function(a,b){return Math.round(Math.random())-0.5}).each(function(i) {
					$(this).hide(); $(this).delay(i*100).fadeTo('200',1);
				});	
				
			});

			jQuery(".activa_items_mobile").click(function(event) {
	
				event.preventDefault();
				var rel = jQuery(this).attr('rel');
				
				for(i=1;i<=total_items;i++) { jQuery('.items_pag'+i).hide(); }
				
				if(rel==1) if(pag_items<total_items) pag_items++; else pag_items = 1;
				else if(pag_items>1) pag_items--; else pag_items = total_items;
				
				$(".items_pag"+pag_items).sort(function(a,b){return Math.round(Math.random())-0.5}).each(function(i) {
					$(this).hide(); $(this).delay(i*100).fadeTo('200',1);
				});	
				
			});

			jQuery(".ver_mas").click(function(event) {
	
				event.preventDefault(); 

				$(this).parent(".texto_corto").parent(".item").children(".texto_largo").show(); 
				$(this).parent(".texto_corto").hide(); 
				
			});

			jQuery(".ver_menos").click(function(event) {
	
				event.preventDefault(); 
				$(this).parent(".texto_largo").parent(".item").children(".texto_corto").show(); 
				$(this).parent(".texto_largo").hide(); 
				
			});

			jQuery('.slick').slick({ autoplay: true, autoplaySpeed: 5000, arrows: false, dots: true, pauseOnHover: false, fade: true, cssEase: 'linear', accessibility: false });
			
			jQuery(".scroll").click(function(e) {
			
				e.preventDefault();
				jQuery(".nav_main").fadeOut(200); 
				var full_url = this.href;
				var parts = full_url.split("#");
				var trgt = parts[1];
				var target_offset = $("#"+trgt).offset();
				var target_top = target_offset.top -90;
				jQuery('html, body').animate({scrollTop:target_top}, 500);
						
			});

			jQuery('.fancybox').fancybox();

		});
		
		function initialize() {
			 
			var coordenadas = new google.maps.LatLng( -24.792793, -65.405501);
			 
			    var colores = [
			        {
			          featureType: "all",
			          elementType: "all",
			          stylers: [
			            { saturation: -100 }
			          ]
			        }
			    ];
			 
			    var opciones = {
			        zoom: 16,
			        center: coordenadas,
			        mapTypeId: google.maps.MapTypeId.ROADMAP
			    };
			 
			    var mapa = new google.maps.Map(document.getElementById('map'), opciones);
			 
			    var estilo = new google.maps.StyledMapType(colores);
			    mapa.mapTypes.set('mapa-bn', estilo);
			    mapa.setMapTypeId('mapa-bn');
		
				var image = 'imgs/seniala.png';
			 
			    var marcador = new google.maps.Marker({
			        position: { lat: -24.792793, lng: -65.405501 },
			        map: mapa,
			        icon: image
			    });
		
			 
		}
		
	</script>

	
</body>

</html>